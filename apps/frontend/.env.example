# Frontend Environment Variables
# Copy this file to .env.frontend and fill in your values

# Node Environment
NODE_ENV=staging

# Frontend URL (public)
NEXT_PUBLIC_APP_URL=http://NaSaSakhiFEStg_IP
NEXT_PUBLIC_API_URL=http://NaSaSakhiFEStg_IP/api

# Backend URL (server-side only, internal network)
BACKEND_URL=http://NaSaSakhiBEStg_INTERNAL_IP:4000

# NextAuth (must match backend exactly)
NEXTAUTH_URL=http://NaSaSakhiFEStg_IP
NEXTAUTH_SECRET=<SAME_AS_BACKEND>

# Optional
NEXT_TELEMETRY_DISABLED=1
